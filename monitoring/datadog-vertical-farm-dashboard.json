            "layout": {
              "x": 2,
              "y": 0,
              "width": 2,
              "height": 2
            }
          },
          {
            "definition": {
              "title": "Active Alerts",
              "type": "query_value",
              "requests": [
                {
                  "formulas": [
                    {
                      "formula": "query1"
                    }
                  ],
                  "queries": [
                    {
                      "query": "sum:vertical_farm.alerts.active{env:production}",
                      "data_source": "metrics",
                      "name": "query1",
                      "aggregator": "last"
                    }
                  ],
                  "response_format": "scalar",
                  "conditional_formats": [
                    {
                      "comparator": ">",
                      "value": 5,
                      "palette": "white_on_red"
                    },
                    {
                      "comparator": ">",
                      "value": 0,
                      "palette": "white_on_yellow"
                    },
                    {
                      "comparator": "<=",
                      "value": 0,
                      "palette": "white_on_green"
                    }
                  ]
                }
              ],
              "autoscale": true,
              "text_align": "center",
              "precision": 0
            },
            "layout": {
              "x": 4,
              "y": 0,
              "width": 2,
              "height": 2
            }
          },
          {
            "definition": {
              "title": "Database Health Score",
              "type": "query_value",
              "requests": [
                {
                  "formulas": [
                    {
                      "formula": "100 - (query1 * 10)"
                    }
                  ],
                  "queries": [
                    {
                      "query": "avg:supabase.database.avg_query_time{env:production}",
                      "data_source": "metrics",
                      "name": "query1",
                      "aggregator": "avg"
                    }
                  ],
                  "response_format": "scalar",
                  "conditional_formats": [
                    {
                      "comparator": ">=",
                      "value": 90,
                      "palette": "white_on_green"
                    },
                    {
                      "comparator": ">=",
                      "value": 70,
                      "palette": "white_on_yellow"
                    },
                    {
                      "comparator": "<",
                      "value": 70,
                      "palette": "white_on_red"
                    }
                  ]
                }
              ],
              "autoscale": false,
              "custom_unit": "%",
              "text_align": "center",
              "precision": 0
            },
            "layout": {
              "x": 6,
              "y": 0,
              "width": 2,
              "height": 2
            }
